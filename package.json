{
  "name": "safe-react-e2e-tests",
  "description": "End to end integration tests for safe-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/gnosis/safe-reacte2e-tests.git"
  },
  "license": "MIT",
  "author": {
    "name": "Gnosis Team",
    "email": "safe@gnosis.io"
  },
  "scripts": {
    "lint:check": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --detectOpenHandles",
    "test-ci": "jest --detectOpenHandles --json --outputFile=output.json",
    "test-local": "cross-env TESTING_ENV=local yarn test",
    "test-pr": "cross-env TESTING_ENV=PR yarn test",
    "format-tests-output": "node ./scripts/format-output.js"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "4",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^11.1.0",
    "pptr-testing-library": "^0.6.5",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "@dasanra/dappeteer": "^2.1.1",
    "puppeteer": "^9.1.1"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "testTimeout": 180000,
    "rootDir": "src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
